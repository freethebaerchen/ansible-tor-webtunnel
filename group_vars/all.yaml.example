admins:
  - name: username
    public-keys:
      - key: "ssh-ed25519 AAAAC3NzaC1lZDI1NTE5AAAAIKW2jR2whDuND6SZPYrX6hU5lCjPdyJ8eYy5qp6xLYQ5 username@example.com"
      - key: "ssh-ed25519 AAAAC3NzaC1lZDI1NTE5AAAAIPAjVNV2L+9Eq+pHpvz4P6JKAJctb0y5f4Z7ieUqFsPC username@example.org"
      - key: "ssh-ed25519 AAAAC3NzaC1lZDI1NTE5AAAAIEWBZo2WYZxQd7szdTHDg8O4CyWwfe+VXsAzobE10v8y username@example.net"

users: []

hcloud:
  sshkey:
    - name: "fallback-key"
      key: "ssh-ed25519 AAAAC3NzaC1lZDI1NTE5AAAAIKW2jR2whDuND6SZPYrX6hU5lCjPdyJ8eYy5qp6xLYQ5"
      state: present
  servers:
    - name: your-bridge-nbg-0
      location: nbg1
      image: ubuntu-24.04
      type: cax11
    - name: your-bridge-fsn-0
      location: fsn1
      image: ubuntu-24.04
      type: cax11
    - name: your-bridge-hel-0
      location: hel1
      image: ubuntu-24.04
      type: cax11

email: user@example.com

deploy_webroot: false
use_webserver: caddy #other possible values are nginx, apache and docker
website:
  no_index: true

tor:
  nickname: somenickname
  distribution_method: Telegram # Possible values for this are documented here: https://gitlab.torproject.org/tpo/anti-censorship/rdsys/-/blob/main/doc/distributors.md